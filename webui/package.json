{
  "name": "tradebot",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "start:dev": "NODE_ENV=development && webpack-dev-server --hot --progress --colors --content-base public/ --config scripts/webpack.config.dev.js",
    "start": "node scripts/server.js",
    "watch": "cross-env NODE_ENV=development webpack --progress --colors -w --config scripts/webpack.config.dev.js",
    "clean": "rimraf public/scripts",
    "build:dev": "npm run clean && cross-env NODE_ENV=development webpack -p --config scripts/webpack.config.dev.js",
    "build:pro": "npm run clean && cross-env NODE_ENV=production webpack -p --config scripts/webpack.config.pro.js"
  },
  "repository": "https://github.com/jeremyhahn/tradebot.git",
  "author": "Jeremy Hahn <mail@jeremyhahn.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^1.1.3",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-loader": "^1.9.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "offline-plugin": "^4.8.5",
    "react-pagination-table": "^1.2.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-livereload-plugin": "^1.0.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "d3-dsv": "^1.0.8",
    "d3-format": "^1.2.1",
    "d3-scale": "^1.0.7",
    "d3-time-format": "^2.1.1",
    "jwt-decode": "^2.2.0",
    "material-ui": "^1.0.0-beta.30",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
